<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Sequential tests">

	<listeners>
		<listener class-name="com.fusioncharts.ExtentReportListener.ExtentReporterNG"/>
	</listeners>

  	<test name="GetObjectReference" parallel="false" preserve-order="true" thread-count="1" >
    	<classes>
      		<class name="com.fusioncharts.apitest.GetObjectReference" />
    	</classes>
  	</test>
  	
  	<test name="Clone" parallel="false" preserve-order="true" thread-count="1" >
    	<classes>
      		<class name="com.fusioncharts.apitest.Clone" />
    	</classes>
  	</test>
  	
</suite>