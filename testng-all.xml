<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="API Regression">

	<listeners>
		<listener class-name="com.fusioncharts.ExtentReportListener.ExtentReporterNG"/>
	</listeners>
  	
  	<test name="Clone" parallel="false" preserve-order="true" thread-count="1" >
    	<classes>
      		<class name="com.fusioncharts.apitest.Clone" />
    	</classes>
  	</test>
  	
  	<test name="ChartType" parallel="false" preserve-order="true" thread-count="1" >
    	<classes>
      		<class name="com.fusioncharts.apitest.ChartType" />
    	</classes>
  	</test>
  	
  	<test name="AddEventListener" parallel="false" preserve-order="true" thread-count="1" >
    	<classes>
      		<class name="com.fusioncharts.apitest.AddEventListener" />
    	</classes>
  	</test>
  	
  	<test name="FormatNumber" parallel="false" preserve-order="true" thread-count="1" >
    	<classes>
      		<class name="com.fusioncharts.apitest.FormatNumber" />
    	</classes>
  	</test>
  	
  	<test name="IsPlotItemSliced_slicePlotItem" parallel="false" preserve-order="true" thread-count="1" >
    	<classes>
      		<class name="com.fusioncharts.apitest.IsPlotItemSliced_slicePlotItem" />
    	</classes>
  	</test>
  	
</suite>